//Archivo de configuracion de reglas eslint

{
    "env": {
        "browser": true
    },
    "rules": {
        // Override our default settings just for this directory
        "eqeqeq": "warn",
        "strict": "off",
        "quotes": ["warn", "double"],
        "semi": ["error", "never"],
        "no-dupe-keys": "error"
    }
}